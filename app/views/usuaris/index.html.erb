<p id="notice"><%= notice %></p>

<h1>Usuaris</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuaris.each do |usuari| %>
      <tr>
        <td><%= link_to 'Show', usuari %></td>
        <td><%= link_to 'Edit', edit_usuari_path(usuari) %></td>
        <td><%= link_to 'Destroy', usuari, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuari', new_usuari_path %>
